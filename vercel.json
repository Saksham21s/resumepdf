{
  "version": 2,
  "functions": {
    "api/*.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "routes": [
    { "src": "/generate-pdf", "dest": "/api/index.js" },
    { "src": "/resume-pdf", "dest": "/api/index.js" },
    { "src": "/render-template", "dest": "/api/index.js" },
    { "src": "/", "dest": "/index.html" }
  ]
}
