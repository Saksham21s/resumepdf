{
  "version": 2,
  "functions": {
    "api/*.js": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "routes": [
    { "src": "/generate-pdf", "dest": "/index.js" },
    { "src": "/resume-pdf", "dest": "/index.js" },
    { "src": "/render-template", "dest": "/index.js" },
    { "src": "/(.*)", "dest": "/index.js" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
} 